[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 = "res://addons/my_ext/bin/linux/debug/libmy_ext.so"
linux.release.x86_64 = "res://addons/my_ext/bin/linux/release/libmy_ext.so"
linux.debug.arm64 = "res://addons/my_ext/bin/linux/debug/libmy_ext.so"
linux.release.arm64 = "res://addons/my_ext/bin/linux/release/libmy_ext.so"

windows.debug.x86_64 = "res://addons/my_ext/bin/windows/debug/my_ext.dll"
windows.release.x86_64 = "res://addons/my_ext/bin/windows/release/my_ext.dll"

macos.debug = "res://addons/my_ext/bin/macos/debug/libmy_ext.dylib"
macos.release = "res://addons/my_ext/bin/macos/release/libmy_ext.dylib"
macos.debug.arm64 = "res://addons/my_ext/bin/macos/debug/libmy_ext.dylib"
macos.release.arm64 = "res://addons/my_ext/bin/macos/release/libmy_ext.dylib"
